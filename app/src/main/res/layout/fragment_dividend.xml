<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".AlertFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:fontFamily="@font/notosansbold"
            android:gravity="center"
            android:text="주식 추가"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp" />

        <Button
            android:id="@+id/button_previous"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/previous"
            app:layout_constraintBottom_toBottomOf="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/searchbar"
            android:fontFamily="@font/notosansbold"
            android:hint="            검색"
            android:includeFontPadding="false"
            android:minWidth="48dp"
            android:minHeight="48dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:textColorHint="#d3b1a8"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            tools:layout_editor_absoluteX="15dp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="35dp"
            android:layout_marginRight="15dp"
            app:layout_constraintEnd_toEndOf="@+id/editText"
            app:layout_constraintStart_toStartOf="@+id/editText"
            app:layout_constraintTop_toBottomOf="@+id/editText">


            <ImageButton
                android:id="@+id/image_237501"
                android:layout_width="95dp"
                android:layout_height="95dp"
                android:background="@drawable/group_237501"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/stockTextName1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosansregular"
                android:includeFontPadding="false"
                android:text="삼성전자"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/image_237501"
                app:layout_constraintStart_toStartOf="@+id/image_237501"
                app:layout_constraintTop_toBottomOf="@+id/image_237501" />


            <ImageButton
                android:id="@+id/group_237462"
                android:layout_width="95dp"
                android:layout_height="95dp"
                android:background="@drawable/group_237462"
                app:layout_constraintEnd_toStartOf="@+id/image_237502"
                app:layout_constraintStart_toEndOf="@+id/image_237501"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/stockTextName2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosansregular"
                android:includeFontPadding="false"
                android:text="SK"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/group_237462"
                app:layout_constraintStart_toStartOf="@+id/group_237462"
                app:layout_constraintTop_toBottomOf="@+id/group_237462" />

            <ImageButton
                android:id="@+id/image_237502"
                android:layout_width="95dp"
                android:layout_height="95dp"
                android:background="@drawable/group_237502"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/stockTextName3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosansregular"
                android:includeFontPadding="false"
                android:text="HD현대"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/image_237502"
                app:layout_constraintStart_toStartOf="@+id/image_237502"
                app:layout_constraintTop_toBottomOf="@+id/image_237502" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>